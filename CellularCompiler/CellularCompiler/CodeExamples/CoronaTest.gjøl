GRID
{
	x: 15;
	y: 15;
}

STATES dead, alive
{
	
}

INITIAL
{
	grid[1,1] = alive;
	grid[1,2] = alive;
	grid[1,3] = alive;
}

#Define rules#
# Rules ved dette shit om cell#
# .state 	#
# .x  		#
# .y 			#
# .floor		#
RULES
{
	match(.state) # This is just a simple "Rule" #
	{
		[alive]
		{
			# Do some counting of neighbors #
			count = 2;
			match (count)
			{
				[2 -> 3]
					return alive;
				[_]
					return dead;
			}
		}
		[dead]
		{
		 	# Do some counting of neighbors #
		 	count = 3;
		 	match (count)
		 	{
		 		[3]
		 			return alive;
		 		[_]
		 			return dead;
		 	}
		}
	}
}