GRID
{
	x: 15;
	y: 15;
}

STATES dead, alive
{
	
}

INITIAL
{
	grid[1,1] = alive;
	grid[2,2] = alive;
	grid[3,3] = alive;
	while(4 == 4)
		grid[4,4] = alive;
}

#Define rules#
# Rules ved dette shit om cell#
# .state 	#
# .x  		#
# .y 			#
# .floor		#
RULES
{
	match(.state, grid[.x-1, .y].state) # This is just a simple "Rule" #
	{
		[dead, alive]
			return alive;
	}

	match(.state, grid[.x+1, .y].state)
	{
		[alive, alive]
			return dead;
	}
}