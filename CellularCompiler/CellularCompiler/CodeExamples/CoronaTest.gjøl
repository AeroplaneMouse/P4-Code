GRID
{
	x: 15;
	y: 15;
}

STATES white, black
{
	
}

INITIAL
{
	grid[1,1] = black;
	grid[1,2] = black;
	grid[0,0] = black;
}

#Define rules#
# Rules ved dette shit om cell#
# .state 	#
# .x  		#
# .y 			#
# .floor		#
RULES
{
	match(.state) # This is just a simple "Rule" #
	{
		[black]
		{
			count = 2;
			match (count)
			{
				[2 -> 3]
					return white;
				[_]
					return black;
			}
		}
		[white]
		{
			return black;
		}
	}
}