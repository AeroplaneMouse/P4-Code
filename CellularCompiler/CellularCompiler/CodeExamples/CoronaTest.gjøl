GRID
{
	x: 15;
	y: 15;
}

STATES dead, alive
{
	
}

INITIAL
{
	grid[1,1] = alive;
	grid[2,2] = alive;
	grid[3,3] = alive;
	grid[5,5] = alive;

	test = 2 != 3;
}

#Define rules#
# Rules ved dette shit om cell#
# .state 	#
# .x  		#
# .y 			#
# .floor		#
RULES
{
	match(.state, grid[0,0]) # This is just a simple "Rule" #
	{
		[dead, dead]
			return alive;
		[alive, dead]
			return dead;
	}
}