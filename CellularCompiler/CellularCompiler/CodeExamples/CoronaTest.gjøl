GRID
{
	x: 15;
	y: 15;
}

STATES dead, alive
{
	
}

INITIAL
{
	grid[1,1] = alive;
	grid[2,2] = alive;
	grid[3,3] = alive;
	grid[8,8] = alive;

	test = 1;
	while(test < 5 + 2)
	{
		test = test + 1;
	}
}

RULES
{
	match(.state, .x, .y) # This is just a simple "Rule" #
	{
		[dead, 5 -> 10, 5 -> 10]
			return alive;
		[alive, 5 -> 10, 5 -> 10]
			return dead;
	}

	match (.state, grid[7, 7])
	{
		[dead, alive]
			return alive;
		[alive, dead]
			return dead;
	}
}