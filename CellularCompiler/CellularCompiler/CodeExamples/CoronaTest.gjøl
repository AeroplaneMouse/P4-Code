GRID
{
	x: 0 -> 15;
	y: 0 -> 15;
}

STATES white, black
{
	
}

INITIAL
{
	grid[1,1] = black;
	grid[1,2] = black;
	#grid[0,1] = 1;#
}

#Define rules#
# Rules ved dette shit om cell#
# state 		#
# .x  		#
# .y 			#
# .floor		#
RULES
{
	match(state) # This is just a "Rule" #
	{
		[black]
		{
			return white;
		}
		[white]
		{
			return black;
		}
	}
}